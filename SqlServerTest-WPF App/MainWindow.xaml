<Window x:Class="SqlServerTest_WPF_App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SqlServerTest_WPF_App"
        mc:Ignorable="d"
        Title="MainWindow" Height="345" Width="430">
    <Grid>
        <Label Content="Sensor Name" HorizontalAlignment="Left" Margin="25,29,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="Normal"/>
        <Label Content="Sensor Type" HorizontalAlignment="Left" Margin="25,0,0,0" VerticalAlignment="Center" FontSize="24" FontWeight="Normal"/>
        <TextBox x:Name="SensorNameTB" HorizontalAlignment="Left" Margin="25,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="196" FontWeight="Normal" FontSize="24"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="281,264,0,0" VerticalAlignment="Top" FontWeight="Normal" FontSize="24" Width="115" Click="Button_Click"/>
        <ComboBox x:Name="ComboSensorType" HorizontalAlignment="Left" Margin="25,198,0,0" VerticalAlignment="Top" Width="196" FontSize="24" FontWeight="Normal"/>

    </Grid>
</Window>
